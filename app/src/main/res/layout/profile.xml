<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/profile"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF">


    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true" >

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <Button
                android:id="@+id/pbutton"
                android:layout_width="262dp"
                android:layout_height="38dp"
                android:layout_marginStart="86dp"
                android:layout_marginTop="28dp"
                android:layout_marginEnd="63dp"
                android:layout_marginBottom="35dp"
                android:text="▼さらに表示する"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView2" />

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="174dp"
                android:layout_height="147dp"
                android:layout_marginStart="15dp"
                android:layout_marginTop="23dp"
                android:layout_marginEnd="16dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/pimageView12"
                app:layout_constraintTop_toBottomOf="@+id/pbutton9"
                app:srcCompat="@drawable/ic_launcher_background" />

            <TextView
                android:id="@+id/ptextView12"
                android:layout_width="246dp"
                android:layout_height="28dp"
                android:layout_marginStart="82dp"
                android:layout_marginTop="31dp"
                android:layout_marginEnd="82dp"
                android:text="名前"
                android:textSize="24sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/pimageView6" />

            <Button
                android:id="@+id/pbutton8"
                android:layout_width="213dp"
                android:layout_height="48dp"
                android:layout_marginTop="42dp"
                android:layout_marginEnd="2dp"
                android:text="自分の投稿"
                app:layout_constraintEnd_toStartOf="@+id/pbutton9"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/ptextView12" />

            <ImageView
                android:id="@+id/pimageView5"
                android:layout_width="190dp"
                android:layout_height="131dp"
                android:layout_marginStart="24dp"
                android:layout_marginTop="12dp"
                android:layout_marginEnd="16dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/imageView2"
                app:layout_constraintTop_toBottomOf="@+id/imageView"
                app:srcCompat="@android:drawable/progress_horizontal" />

            <ImageView
                android:id="@+id/pimageView6"
                android:layout_width="158dp"
                android:layout_height="145dp"
                android:layout_marginStart="124dp"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="8dp"
                app:layout_constraintEnd_toStartOf="@+id/pbutton7"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@android:drawable/ic_menu_info_details" />

            <Button
                android:id="@+id/pbutton9"

                android:layout_width="196dp"
                android:layout_height="49dp"
                android:layout_marginStart="2dp"
                android:layout_marginTop="42dp"
                android:text="お気に入り"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/pbutton8"
                app:layout_constraintTop_toBottomOf="@+id/ptextView12" />

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="145dp"
                android:layout_height="163dp"
                android:layout_marginStart="39dp"
                android:layout_marginTop="2dp"
                android:layout_marginEnd="24dp"
                app:layout_constraintEnd_toStartOf="@+id/pimageView5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/pimageView12"
                app:srcCompat="@android:drawable/checkbox_on_background" />

            <ImageView
                android:id="@+id/pimageView12"
                android:layout_width="190dp"
                android:layout_height="131dp"
                android:layout_marginStart="24dp"
                android:layout_marginTop="24dp"
                android:layout_marginEnd="8dp"
                app:layout_constraintEnd_toStartOf="@+id/imageView"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/pbutton8"
                app:srcCompat="@android:drawable/progress_horizontal" />

            <Button
                android:id="@+id/pbutton7"
                android:layout_width="87dp"
                android:layout_height="82dp"
                android:layout_marginStart="24dp"
                android:layout_marginTop="56dp"
                android:layout_marginEnd="24dp"
                android:text="編集画面へ"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/pimageView6"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.core.widget.NestedScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>